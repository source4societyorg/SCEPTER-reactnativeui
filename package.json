{
  "name": "changeme",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage"
  },
  "dependencies": {
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.0"
  },
  "devDependencies": {
    "babel-jest": "22.4.1",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.2",
    "jest-junit": "^3.6.0",
    "react-test-renderer": "^16.3.0-alpha.1"
  },
  "jest": {
    "preset": "react-native",
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "testResultsProcessor": "jest-junit",
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  }
}